class A {
    string to_string() {
        return "A";
    }
}

class B extends A {
    string to_string() {
        return "B";
    }
}

class C {

}

class D extends C {

}

int main() {
    C x = new D();
    println(x); // x != null && real(x) = D

    B b = new B();
    println(b);             // B
    println(b.to_string()); // B
   
    b = null;
    println(b);             // null
    println(b.to_string()); // null pointer error

    return 0;
}