
// explicit constructor
// with inheretance

class A {
    int a;

    A() {
        a = 100;
    }
}

class B extends A {
    string b;

    B() {
        this(10, "hello");
    }

    B(int a, string b) {
        super();

        println(this.a);

        this.a = a;
        this.b = b;
    }
}

int main() {    
    B x1 = new B();
    println(x1.a);
    println(x1.b);

    return 0;
}