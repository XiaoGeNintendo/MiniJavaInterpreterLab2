

// explicit constructor

class A {
    int a;
    string b;

    A() {
        // default value for a and b
    }

    A(int a, string b) {
        this.a = a;
        this.b = b;
    }
}
int main() {
    A a1 = new A();

    println(a1.a);
    println(a1.b);

    A a2 = new A(20, "world");
    println(a2.a);
    println(a2.b);

    return 0;
}